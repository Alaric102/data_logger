<?xml version="1.0" encoding="utf-8"?>
<launch>
	<arg name="topic" default="cloud" />
    <arg name="output_path" default="Workspace" />
    <node pkg="data_logger" type="cloud_logger_node" name="cloud_logger_node" output="screen" required="true">
            <param name="topic" value="$(arg topic)" />
            <param name="output_path" value="$(arg output_path)" />
    </node>
</launch>
